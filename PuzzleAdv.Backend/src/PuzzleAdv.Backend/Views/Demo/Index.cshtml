@model IEnumerable<PuzzleAdv.Backend.Models.Puzzle>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeleteDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeleteUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Distance)
