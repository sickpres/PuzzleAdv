        <td>
            @Html.DisplayFor(modelItem => item.DeleteDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeleteUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Distance)
